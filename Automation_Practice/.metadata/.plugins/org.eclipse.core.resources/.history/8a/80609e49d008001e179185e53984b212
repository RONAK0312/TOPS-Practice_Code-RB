package com.core;


class Student1
{
	int rno;
	String sname;
	double per;
	public Student1(int rno, String sname, double per) 
	{
		this.rno = rno;
		this.sname = sname;
		this.per = per;
	}
	public String toString()
	{
		return "Roll no : "+rno+"\nName of student :"+sname+"\nPercentage :"+per;
	}
	
	
}

public class Serialization {

}
